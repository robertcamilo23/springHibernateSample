<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="Comp473p2.domain">
    <class name="Occupancy" table="Occupancies">
        <id name="occupancyId" column="id">
            <generator class="native"/>
        </id>
        <property name="startDate" type="java.util.Date" column="startDate"/>
        <property name="endDate" type="java.util.Date" column="endDate"/>
        <property name="usage" type="java.lang.String" column="usage"/>
        <property name="totalCapacity" type="java.lang.Integer" column="totalCapacity"/>
    </class>
</hibernate-mapping>